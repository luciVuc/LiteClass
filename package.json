{
  "name": "liteclass",
  "version": "1.0.1",
  "description": "JS module that aims to facilitate object-oriented programming (OOP) in JavaScript by simplifying the process of creating and extending classes through prototypal inheritance.",
  "main": "src/class.js",
  "private": false,
  "scripts": {
    "make": "browserify src/class.js --standalone LiteClass -o dist/class.js",
    "prepublish": "npm run make"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/luciVuc/LiteClass.git"
  },
  "keywords": [
    "javascript",
    "liteclass js",
    "oop",
    "object oriented js",
    "oop js",
    "js class",
    "inheritance",
    "extend"
  ],
  "author": "Lucian Vuc <lucian.vuc@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucivuc/LiteClass/issues"
  },
  "dependencies": {
    "events": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "qunitjs": "^2.0.1"
  }
}
